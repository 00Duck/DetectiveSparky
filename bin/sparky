#!/Users/blake/Documents/Programs/DetectiveSparky/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'sparky','console_scripts','sparky'
__requires__ = 'sparky'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('sparky', 'console_scripts', 'sparky')()
    )
